{
  "containerDefinitions": [
    {
      "name": "deleteapp",
      "image": "id.dkr.ecr.us-east-1.amazonaws.com/miapp:v1",
      "cpu": 512,
      "memoryReservation": 512,
      "dnsSearchDomains": ["miapp.internal"],
      "portMappings": [{
        "hostPort": 0,
        "containerPort": 8080,
        "protocol": "tcp"
      }],
      "environment" : [
        { "name" : "REDIS_DB", "value" : "redis.miapp.internal:6379" }
      ],
      "essential": true
    }
  ],
  "family": "deleteapp"
}